{
  "rewrites": [
    {
      "source": "/empleo",
      "destination": "/empleo/index.html"
    },
    {
      "source": "/admin-vacantes",
      "destination": "/admin-vacantes/index.html"
    },
    {
      "source": "/about",
      "destination": "/about/index.html"
    },
    {
      "source": "/contact",
      "destination": "/contact/index.html"
    },
    {
      "source": "/gallery",
      "destination": "/gallery/index.html"
    },
    {
      "source": "/productos",
      "destination": "/productos/index.html"
    },
    {
      "source": "/productos-terceros",
      "destination": "/productos-terceros/index.html"
    },
    {
      "source": "/politicas-seguridad-alimentaria",
      "destination": "/politicas-seguridad-alimentaria/index.html"
    },
    {
      "source": "/dist-lunch",
      "destination": "/dist-lunch/index.html"
    }
  ],
  "redirects": [
    {
      "source": "/about.html",
      "destination": "/about",
      "permanent": true
    },
    {
      "source": "/contact.html",
      "destination": "/contact",
      "permanent": true
    },
    {
      "source": "/gallery.html",
      "destination": "/gallery",
      "permanent": true
    },
    {
      "source": "/productos.html",
      "destination": "/productos",
      "permanent": true
    },
    {
      "source": "/productos-terceros.html",
      "destination": "/productos-terceros",
      "permanent": true
    },
    {
      "source": "/empleo.html",
      "destination": "/empleo",
      "permanent": true
    },
    {
      "source": "/admin-vacantes.html",
      "destination": "/admin-vacantes",
      "permanent": true
    },
    {
      "source": "/politicas-seguridad-alimentaria.html",
      "destination": "/politicas-seguridad-alimentaria",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/(.*\\.(js|css|jpg|jpeg|png|gif|svg|webp|woff|woff2|ttf|eot|ico))",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*\\.html)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600, must-revalidate"
        }
      ]
    },
    {
      "source": "/images/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
